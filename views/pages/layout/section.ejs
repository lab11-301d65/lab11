
<section>
  <h2>Total books saved: <%= numberOfBooksInDataBase %></h2>
  <ul>
    <% for(let i = 0; i < books.length; i++){ %>
      <div>
        <img src='<%= books[i].image_url%>'>
        <h1><%= books[i].title %></h1>
        <h2><%= books[i].author %></h2>
        <p><%= books[i].description %></p>
        <p><%= books[i].isbn %></p>
      </div>
      <% } %>
    </ul>
</section>